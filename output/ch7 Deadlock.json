    },
    {
      "level": "H2",
      "text": "false",
      "page": 52
    },
    {
      "level": "H2",
      "text": ", then",
      "page": 52
    },
    {
      "level": "H2",
      "text": "P",
      "page": 52
    },
    {
      "level": "H2",
      "text": "is deadlocked",
      "page": 52
